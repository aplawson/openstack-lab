# Node VM configuration. Used by osbash (host and guest).

# Port forwarding
VM_SSH_PORT=2232

# Assign network interfaces to networks
NET_IF_0=dhcp
# Mgmt network
NET_IF_1="static 10.0.0.31"
# Tunnel network
NET_IF_2="static 10.0.1.31"

# A default instance within compute uses 512 MB RAM
VM_MEM=1024

# Override number of virtual CPUs (default is 1)
VM_CPUS=2
