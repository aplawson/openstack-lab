# Node VM configuration. Used by osbash (host and guest).

# Port forwarding
VM_SSH_PORT=2231

# Assign network interfaces to networks
NET_IF_0=dhcp
# Mgmt network
NET_IF_1="static 10.0.0.21"
# Tunnel network
NET_IF_2="static 10.0.1.21"
# API/external network
NET_IF_3="static 203.0.113.21"

# A default instance within network uses 512 MB RAM
# VM_MEM=512

# Override number of virtual CPUs (default is 1)
# Uncomment and edit the line below to change number of vCPUs used
# VM_CPUS=1
